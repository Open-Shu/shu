{
  "name": "shu-admin-frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.0",
    "axios": "^1.4.0",
    "cron-parser": "^5.4.0",
    "cronstrue": "^3.9.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "js-yaml": "^4.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-js-cron": "^5.2.0",
    "react-json-pretty": "^2.2.0",
    "react-markdown": "^10.1.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.11.2",
    "remark-gfm": "^4.0.1"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:watch": "vitest --coverage",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,md}\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "src/**/*.{js,jsx,ts,tsx}"
        ],
        "excludedFiles": [
          "src/services/api.js"
        ],
        "rules": {
          "no-restricted-syntax": [
            "error",
            {
              "selector": "MemberExpression[property.name='data'][object.type='MemberExpression'][object.property.name='data']",
              "message": "Avoid response.data.data. Use extractDataFromResponse from services/api."
            },
            {
              "selector": "MemberExpression[property.name='success'][object.type='MemberExpression'][object.property.name='data']",
              "message": "Avoid checking response.data.success. Use HTTP status and extractDataFromResponse/formatError."
            }
          ]
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8000",
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.0.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.1",
    "vite": "^5.0.8",
    "vitest": "^1.0.4"
  }
}
